snippet buttonmixin
	@mixin button($color, $bg) {
	  display: inline-block;
	  padding: 18px 30px;
	  color: $color;
	  background: $bgcolor;
	  border-radius: 4px;
	  text-align: center;
	  font-size: 20px;
	  font-weight: bold;
	  line-height: 24px;
	  transition: all .3s ease;

	  &:hover {
	    background: $color;
	    color: $bg;
	  }

	  @include small-down {
	    padding: 15px 22px;
	  }
	}
