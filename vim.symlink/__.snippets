snippet jqueryplugin
	(function ($, window, document, undefined) {
	  var pluginName = 'shinnSlider';

	  function ShinnSlider (el, options, sel) {
	    this.$el = $(el);
	    this.$sel = sel;

	    var defaults = {
	      activeClass: 'active-news-article',
	      animationSpeed: 750,
	      debug: false
	    };

	    this.options = $.extend({}, defaults, options);

	    this.init();
	  }

	  ShinnSlider.prototype = {

	    // Initialize children and events
	    init: function () {
	      var plugin = this;

	      plugin.container = plugin.$el.find('ul');
	      plugin.leftButton = plugin.$el.find('.slider-nav--prev');
	      plugin.rightButton = plugin.$el.find('.slider-nav--next');
	      plugin.liswidth = 0;

	      plugin.initEvents();
	    },

	    // Events here
	    initEvents: function () {
	      var plugin = this;
	      console.log(plugin);

	      $(plugin.container).on('scroll load', function () {
	        plugin.updatePositions();
	      });
	    },

	    prevSlide: function (e) {
	      var plugin = this;
	    },

	    checkArrows: function () {
	      var plugin = this;
	    },

	    updatePositions: function () {
	    }

	  };

	  $.fn[pluginName] = function (options) {
	    var sel = this.selector;
	    return this.each(function () {
	      if (!$.data(this, pluginName)) {
	        $.data(this, pluginName, new ShinnSlider(this, options, sel));
	      }
	    });
	  };
	})(jQuery, window, document);
