	function wrapWithFirstLink(elem) {
	  var link = elem.find('a');
	  var archiveLink = link.attr('href');
	  if (link.attr('target') !== undefined) {
	    archiveLink += ' target="_blank"';
	  }
	  if (link.attr('rel') !== undefined) {
	    archiveLink += ' rel="noopener noreferer"';
	  }
	  elem.wrap('<a href=' + archiveLink + '/>')
	}
